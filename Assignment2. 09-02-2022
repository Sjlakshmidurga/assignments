#include<stdio.h>
void main()
{
int arr[10],n,i,sum=0;
int *ptr=arr;
printf("Enter the number of elements to be stored:");
scanf("%d",&n);
printf("\nEnter %d Elements:",n);
for(i=0;i<n;i++)
scanf("%d",&arr[i]);
for (i=0;i<n;i++)
{
sum=sum+*ptr;
*ptr++;
}
printf("\nSum of array numbers is = %d",sum);
}
